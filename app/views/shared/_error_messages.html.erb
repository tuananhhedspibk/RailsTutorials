<% if @user.errors.any? %>
  <div class="errorexplanation">
    <div class="alert alert-danger">
      <%= t "form_contain" %> <%= pluralize @user.errors.count, t("error") %>
    </div>
    <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>	
    </ul>
  </div>
<% end %>
